{
    "name": "dinara/email-marketing",
    "description": "Email marketing package for Laravel with campaigns, templates, open/click tracking, and rate limiting",
    "keywords": [
        "laravel",
        "email",
        "marketing",
        "campaigns",
        "newsletter",
        "mailing",
        "tracking",
        "email-tracking",
        "bulk-email"
    ],
    "homepage": "https://github.com/dinara/email-marketing",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dinara",
            "email": "dinara@riva24.ru",
            "homepage": "https://riva24.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/dinara/email-marketing/issues",
        "source": "https://github.com/dinara/email-marketing"
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/mail": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/queue": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Dinara\\EmailMarketing\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dinara\\EmailMarketing\\EmailMarketingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
